{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "moment.js",
            "background.js"
        ]
    }
}