{
    "manifest_version": 2,
    "name": "Stash",
    "version": "0.2",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "scripts": [
            "moment.js",
            "background.js"
        ]
    }
}